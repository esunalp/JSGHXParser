  {
    "category": "Surface",
    "subcategory": "SubD",
    "name": "MultiPipe",
    "nickname": "MP",
    "guid": "4bfe1bf6-fbc9-4ad2-bf28-a7402e1392ee",
    "description": "Create a branching pipe around a network of lines/curves",
    "library": {
      "name": "Kangaroo2 Components",
      "id": "c2ea695e-1a09-6f42-266d-113498879f60",
      "version": "2.5.3"
    },
    "instantiated": true,
    "inputs": [
      {
        "name": "Curves",
        "nickname": "Curves",
        "access": "list",
        "description": "The curves to pipe. Also accepts meshes"
      },
      {
        "name": "NodeSize",
        "nickname": "NodeSize",
        "access": "list",
        "description": "Pipe radius. If one value given, it is applied to all. Alternatively, provide a list of radii corresponding to each point in SizePoints"
      },
      {
        "name": "SizePoints",
        "nickname": "SizePoints",
        "access": "list",
        "description": "If you are supplying multiple radii for NodeSize, these points identify which node to set as which radius. If only some of the nodes have their radius set this way, the values will be interpolated across the shape"
      },
      {
        "name": "EndOffset",
        "nickname": "EndOffset",
        "access": "item",
        "description": "The distance of the first edge loop away from the node as a multiplier of NodeSize. If this is set to zero, no intermediate edge loop is added, to give a smoother shape."
      },
      {
        "name": "StrutSize",
        "nickname": "StrutSize",
        "access": "item",
        "description": "The size of the struts between nodes as a multiplier of NodeSize. <1 gives tapering struts, >1 gives bulging struts"
      },
      {
        "name": "Segment",
        "nickname": "Segment",
        "access": "item",
        "description": "Approximate spacing of edge loops along each strut. If set to zero, no additional edge loops are added"
      },
      {
        "name": "KinkAngle",
        "nickname": "KinkAngle",
        "access": "item",
        "description": "When the input to 'Curves' are smooth curves, this sets the maximum angle between consecutive segments when discretizing"
      },
      {
        "name": "CubeFit",
        "nickname": "CubeFit",
        "access": "item",
        "description": "If >0 this attempts to fit a cube at each node. Should be a value between 0 and 1, where 0 = never, and 1 = always, depending on how close to orthogonal its connected lines are."
      },
      {
        "name": "Caps",
        "nickname": "Caps",
        "access": "item",
        "description": "Cap option - 0:None, 1:Round, 2:Flat"
      }
    ],
    "outputs": [
      {
        "name": "Pipe",
        "nickname": "P",
        "access": "item",
        "description": "Resulting Pipe SubD"
      }
    ]
  },
